{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "preserveAllComments": false,
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": false,
      "decorators": false
    },
    "target": "es2016",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": true,
    "baseUrl": "./"
  },
  "sourceMaps": "inline",
  "minify": true
}
